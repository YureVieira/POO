	ArrayList<Reptil> setorR = new ArrayList<Reptil>();;
	ArrayList<Ave> setorAv = new ArrayList<Ave>();
	ArrayList<Anfibio> setorAn = new ArrayList<Anfibio>;
	ArrayList<Invertebrado> setorI;
	
	void initSetores(){
		setorM = new ArrayList<Mamifero>();
		setorAv = new ArrayList<Ave>();
		setorR = new ArrayList<Reptil>();
		setorAn = new ArrayList<Anfibio>();
		setorI = new ArrayList<Invertebrado>();
		//como lincar um tratador ao setor? 
	}
}
