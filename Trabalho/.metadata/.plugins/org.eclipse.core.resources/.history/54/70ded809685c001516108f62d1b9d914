
public abstract class  Animal {
	String nome;
	String tipo_alimentacao;
	int idade;
	
	public Animal(String nome, String tipo_alimentacao, int idade) {
		super();
		this.nome = nome;
		this.tipo_alimentacao = tipo_alimentacao;
		this.idade = idade;
	}
	
	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public String getTipo_alimentacao() {
		return tipo_alimentacao;
	}

	public void setTipo_alimentacao(String tipo_alimentacao) {
		this.tipo_alimentacao = tipo_alimentacao;
	}

	public int getIdade() {
		return idade;
	}

	public void setIdade(int idade) {
		this.idade = idade;
	}

	void Comer(){
		System.out.println("Comendo");		
	}
}
